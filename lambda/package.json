{
  "name": "@byu-oit/schema-migrations-lambda",
  "version": "1.0.0",
  "description": "Terraform module that creates a generic lambda function that will run database schema migrations using Umzug",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "del-cli dist",
    "lint": "ts-standard",
    "test": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/byu-oit/terraform-aws-schema-migrations-lambda.git"
  },
  "keywords": [
    "schema",
    "migrations",
    "lambda",
    "aws"
  ],
  "author": "Spencer Tuft <stuft2@byu.edu>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/byu-oit/terraform-aws-schema-migrations-lambda/issues"
  },
  "homepage": "https://github.com/byu-oit/terraform-aws-schema-migrations-lambda#readme",
  "devDependencies": {
    "@tsconfig/node14": "^1.0.0",
    "@types/debug": "^4.1.5",
    "@types/mysql": "^2.15.18",
    "@types/node": "^14.14.31",
    "@types/pg": "^7.14.11",
    "aws-sdk": "^2.854.0",
    "del-cli": "^3.0.1",
    "ts-standard": "^10.0.0",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "@aws-sdk/client-codedeploy": "^3.7.0",
    "@aws-sdk/client-s3": "^3.7.0",
    "env-var": "^7.0.0",
    "mysql": "^2.18.1",
    "pg": "^8.5.1",
    "umzug": "^3.0.0-beta.15"
  },
  "ts-standard": {
    "env": [
      "node"
    ],
    "ignore": [
      "dist",
      "node_modules"
    ],
    "fix": true
  }
}
